
#include<stdio.h>
int main(){
    int num[10];
	int i;
	for(i=0;i<=9;i++){
	scanf("%d",&num[i]);
	}
	int max = num[0];
    for(i =1;i<=9;i++){
		max = num[i]>max?num[i]:max; 
	}
	int min = num[0];
    for(i =1;i<=9;i++){
		min = num[i]<min?num[i]:min; 
	}
     printf("sum=%3d",max+min);
return 0;
}
