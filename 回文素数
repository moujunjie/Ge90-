#include<stdio.h>
#include<math.h>
int IsPrime(int x);
int main()
{                   
    int i, j, k, t, m, n, count = 0;
    printf("Input n:\n");
    scanf("%d", &n);
    for (m = 10; m < n; m++)  
    {                   
        i = m / 100;                  
        j = (m - i * 100) / 10;  
        k = m % 10;                
        if (m < 100)             
        {                   
            t = k * 10 + j ;      
        }
        else                        
        {                   
            t = k * 100 + j * 10 + i;
        }
        if (m == t && IsPrime(m))
        {                   
            printf("%4d", m);
            count++;
        }
    }
    printf("\ncount=%d\n", count);
    return 0;
}                   

int IsPrime(int x)
{                   
    int i, flag = 1;
    int squareRoot = (int)sqrt(x);
    if (x <= 1)   flag = 0;     
    for (i = 2; i <= squareRoot && flag; i++)
    {                   
        if (x % i == 0) flag = 0; 
    }
    return flag;
}
